using System;
using System.Collections.Generic;

 class Student
    {
      string? name;
       int age;
      public string? Name  //Property create for write
      {
        set{name=value;}
        get{return name;}
      }
      public int Age   //property create for read
      {
        set{age=value;}
        get{return age;}
      }
    }
class Program
{
    static void Main()
    {
 Student s1=new Student();

 //Write by using propery
 s1.Name="Deloar";
 s1.Age=20;

 Console.WriteLine($"Name={s1.Name} and age={s1.Age}");

    }
}