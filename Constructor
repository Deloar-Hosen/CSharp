using System;
using System.Collections.Generic;


    class Student
    {
        //Declare variable
        public string? name;
        public int roll;
 
   //creating paramiterized constructor for initialize data
    public Student(string name,int roll)
    {
      this.name=name;
      this.roll=roll;
    }

    //creating default constructor for initialize data
        public Student()
    {
     name="salam";
     roll=43;
    }
      
      //creating method for access data
        public void Display()
        {
             Console.WriteLine($"\nStudent name:{name} Roll:{roll}");  
        }
    }

class Program
{
    static void Main()
    {
        //paramitarized constructor
  Student s1=new Student("Deloar",59);
  s1.Display();

//default constructor
  Student s2=new Student();
  s2.Display();


    }
}
