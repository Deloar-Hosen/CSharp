using System;
using System.Collections.Generic;

class Program
{
    class Student
    {
        public string? name;
        public int roll;
    }
    static void Main()
    {
   Student s1=new Student();
   s1.name="Deloar";
   s1.roll=45;
   Console.WriteLine($"\nStudent name:{s1.name} Roll:{s1.roll}");
   
   Student s2=new Student();
   s2.name="Sobuj";
   s2.roll=65;
   Console.WriteLine($"\nStudent name:{s2.name} Roll:{s2.roll}\n");



    }
}
