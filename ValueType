using System;
using System.Collections.Generic;
using System.Security.Cryptography;

class Program
{
    static void Main()
    {
        //Value Type
    int x=5;
    Console.WriteLine($"Before refering x={x}");
    
    int y=x;
    y=3;
    Console.WriteLine($"after refering x={x} and y={y}");


    }
}

