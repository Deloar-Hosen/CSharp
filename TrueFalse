using System;

public class Test
{
    public static void Main()
    {
    int num=-10;
    
    if(num>=0)
    {
        Console.WriteLine($"Positive number.");  
    }
    else
    {
        Console.WriteLine($"Negative number.");   
    }
    //condition?true:false
    string result=num>=0?"Positive number":"Negative number.";
    Console.WriteLine($"\t{result}");

    }
}
