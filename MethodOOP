using System;
using System.Collections.Generic;

class Program
{
    class Student
    {
        //Declare variable
        public string? name;
        public int roll;
 
        //creating method for initialize data
    public void SetData(string name,int roll)
    {
      this.name=name;
      this.roll=roll;
    }
      
      //creating method for access data
        public void Display()
        {
             Console.WriteLine($"\nStudent name:{name} Roll:{roll}");  
        }
    }
    static void Main()
    {
        //creat object
   Student s1=new Student();

   //Calling method from class
   s1.SetData("Deloar",45);
   s1.Display();
   
   Student s2=new Student();
   s2.SetData("Sobuj",64);
   s2.Display();



    }
}
